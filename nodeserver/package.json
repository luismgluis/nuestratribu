    "firebase-admin": "^10.0.0",
    "mongoose": "^6.0.12"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "chai": "^4.2.0",
    "mocha": "^9.0.0",
    "nodemon": "^2.0.14",
    "tslint": "^6.0.0",
    "typescript": "^4.4.4",
    "wait-port": "^0.2.7"
  }
}
